# LiteLLM Configuration for Trading RAG Backend
# Copy this file to .env and replace with your actual values

# OpenAI Configuration for LiteLLM
OPENAI_API_KEY=your_litellm_api_key_here
API_BASE_URL=https://litellm.deriv.ai/v1
OPENAI_MODEL_NAME=your_model_name_here

# Embedding Model Configuration
EMBEDDING_MODEL=text-embedding-3-small

# Example configurations:
# For Deriv LiteLLM:
# OPENAI_API_KEY=your_deriv_litellm_api_key
# API_BASE_URL=https://litellm.deriv.ai/v1
# OPENAI_MODEL_NAME=gpt-4o-mini
# EMBEDDING_MODEL=text-embedding-3-small

# Application Settings (optional)
LOG_LEVEL=INFO
ENVIRONMENT=development 